<?xml version="1.0" encoding="UTF-8"?>
<testing name="Окупаційний режим в Україні">
  <question level="1" simple="True" text="Як називалися рейхскомісаріати, запровадженні нацистським командуванням на окупованих землях Радянського Союзу?">
    <answer flag="1" text="&quot;Україна&quot;"/>
    <answer flag="0" text="&quot;Трансністрія&quot;"/>
    <answer flag="1" text="&quot;Остланд&quot;"/>
  </question>
  <question level="1" simple="True" text="В якому місті перебувала гітлерівська адміністрація окупованої України?">
    <answer flag="0" text="Варшава"/>
    <answer flag="0" text="Одеса"/>
    <answer flag="1" text="Рівне"/>
  </question>
  <question level="1" simple="True" text="Який регіон був включений до Генерального губернаторства (Польща)?">
    <answer flag="0" text="Волинь"/>
    <answer flag="0" text="Правобережжя"/>
    <answer flag="0" text="Полтавщина"/>
    <answer flag="1" text="Східна Галичина"/>
    <answer flag="0" text="Північна Буковина"/>
    <answer flag="0" text="Бессарабія"/>
  </question>
  <question level="1" simple="True" text="Коли була укладена німецько-румунська угода про передачу міжиріччя Дністра і Південного Бугу з містом Одесою?">
    <answer flag="1" text="19 липня 1941 р."/>
    <answer flag="0" text="19 вересня 1941 р."/>
    <answer flag="0" text="19 серпня 1941 р."/>
  </question>
  <question level="1" simple="True" text="В яких містах пройшли наймасовіші розстріли багатолюдних єврейських громад?">
    <answer flag="0" text="Жмеринка"/>
    <answer flag="0" text="Бердичів"/>
    <answer flag="1" text="Янів"/>
    <answer flag="1" text="Київ"/>
  </question>
  <question level="1" simple="True" text="Біля якого міста розміщувалася Ставка Гітлера (&quot;Вервольф&quot;) в Україні?">
    <answer flag="0" text="Київ"/>
    <answer flag="1" text="Вінниця"/>
    <answer flag="0" text="Рівне"/>
    <answer flag="0" text="Вапнярка"/>
  </question>
</testing>
