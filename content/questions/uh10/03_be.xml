<testing name="Украіна ў барацьбе за захаванне дзяржаўнай незалежнасці">
  <question text="Працоўны кангрэс быў">
    <answer flag="0" text="вышэйшым выканаўчым органам УНР"/>
    <answer flag="1" text="вышэйшым заканадаўчым органам УНР"/>
    <answer flag="0" text="дарадчым органам пры атаману С. Пятлюры"/>
    <answer flag="0" text="выконваў функцыю саветаў у Украіне"/>
  </question>
  <question text="Чертковская наступальная аперацыя (офензива) Уга была накіравана">
    <answer flag="1" text="супраць польскіх войскаў"/>
    <answer flag="0" text="супраць расейскіх войскаў"/>
    <answer flag="0" text="супраць частак Чырвонай арміі"/>
    <answer flag="0" text="супраць румынскіх войскаў"/>
  </question>
  <question text="Акт Злучэнні УНР і ЗУНР была абвешчаная">
    <answer flag="0" text="22 Студзень 1919 года ў Львове"/>
    <answer flag="1" text="22 Студзень 1919 года ў Кіеве"/>
    <answer flag="0" text="7 (22) лістапада 1917 у Кіеве"/>
    <answer flag="0" text="1 лістапада 1918"/>
  </question>
  <question text="Галоўнай ударнай сілай войскаў Дырэкторыі падчас антигетманского паўстання былі">
    <answer flag="1" text="УСС"/>
    <answer flag="0" text="Уга"/>
    <answer flag="0" text="Першы ўкраінскі корпус"/>
    <answer flag="0" text="сялянскія паўстанцкія атрады"/>
  </question>
  <question text="З пералічаных асоб выберыце лідэра сялянскага паўстанцкага руху ва Ўкраіне:">
    <answer flag="1" text="Н. Махно"/>
    <answer flag="0" text="А. Дзянікін"/>
    <answer flag="0" text="Е. Петрушевич"/>
    <answer flag="0" text="Х. Ракаўскі"/>
  </question>
  <question text="Пасля ўступлення Чырвонай арміі ў Галіцыю падчас савецка-польскай вайны тут быў створаны">
    <answer flag="0" text="Заходнеўкраінскі Народная Рэспубліка"/>
    <answer flag="0" text="Украінскую Сацыялістычную Савецкую Рэспубліку"/>
    <answer flag="1" text="Галіцкую Сацыялістычную Савецкую Рэспубліку"/>
    <answer flag="0" text="Галіцка-Букавінская генерал-губернатарства"/>
    <answer flag="0" text="Букавінская Савецкую Рэспубліку"/>
  </question>
  <question text="Пасля адначасовага ўступлення ў Кіеў украінскі і белагвардзейскіх войскаў 30 жніўня 1919 г.">
    <answer flag="1" text="ўкраінскія войскі былі вымушаныя пакінуць горад"/>
    <answer flag="0" text="Украінскі войскі выціснулі белагвардзейцаў з горада"/>
    <answer flag="0" text="Кіеў быў падзелены на зоны кантролю"/>
  </question>
  <question text="Як называлася ўкраінскае дзяржава за гетманам П. Скоропадского?">
    <answer flag="0" text="Украінская Народная Рэспубліка"/>
    <answer flag="0" text="Заходнеўкраінскі Народная Рэспубліка"/>
    <answer flag="1" text="Украінскі Дзяржава"/>
  </question>
  <question text="З якой дзяржавай ЗУНР прыйшлося весці ўзброеную барацьбу за сваю незалежнасць?">
    <answer flag="0" text="з Расеяй"/>
    <answer flag="1" text="з Польшчай"/>
    <answer flag="0" text="з Аўстрыяй"/>
    <answer flag="0" text="з Германіяй"/>
  </question>
  <question text="Як называлася палітыка, яе ўвялі бальшавікі ў 1919 г.?">
    <answer flag="0" text="Нэп"/>
    <answer flag="1" text="&quot;Ваенны камунізм&quot;"/>
    <answer flag="0" text="Адраджэнне"/>
  </question>
  <question text="Ключавыя пазіцыі абароны белагвардзейцаў у Крыму знаходзіліся">
    <answer flag="1" text="на Перакопскай пярэсмыку"/>
    <answer flag="0" text="у гарах Крыму"/>
    <answer flag="0" text="у раёне Севастопаля"/>
  </question>
  <question text="Другі &quot;зімовы паход&quot; завяршыўся трагедыяй у раёне горада">
    <answer flag="0" text="Стромкія"/>
    <answer flag="1" text="Базар"/>
    <answer flag="0" text="Кіеў"/>
  </question>
</testing>
